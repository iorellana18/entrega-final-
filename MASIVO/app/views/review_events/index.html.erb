<h1>Listing review_events</h1>

<table>
  <thead>
    <tr>
      <th>Start date review</th>
      <th>Ending date review</th>
      <th>Num of mentions</th>
      <th>Num active users</th>
      <th>Passing rate</th>
      <th>Num mentions positive sum</th>
      <th>Num mentions negative sum</th>
      <th>Event</th>
      <th>Review account event</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @review_events.each do |review_event| %>
      <tr>
        <td><%= review_event.start_date_review %></td>
        <td><%= review_event.ending_date_review %></td>
        <td><%= review_event.num_of_mentions %></td>
        <td><%= review_event.num_active_users %></td>
        <td><%= review_event.passing_rate %></td>
        <td><%= review_event.num_mentions_positive_sum %></td>
        <td><%= review_event.num_mentions_negative_sum %></td>
        <td><%= review_event.event_id %></td>
        <td><%= review_event.review_account_event_id %></td>
        <td><%= link_to 'Show', review_event %></td>
        <td><%= link_to 'Edit', edit_review_event_path(review_event) %></td>
        <td><%= link_to 'Destroy', review_event, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Review event', new_review_event_path %>
