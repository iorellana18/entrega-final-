<h1>Listing evaluation_account_events</h1>

<table>
  <thead>
    <tr>
      <th>Num followers start</th>
      <th>Num followers end</th>
      <th>Evaluation</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @evaluation_account_events.each do |evaluation_account_event| %>
      <tr>
        <td><%= evaluation_account_event.num_followers_start %></td>
        <td><%= evaluation_account_event.num_followers_end %></td>
        <td><%= evaluation_account_event.evaluation_id %></td>
        <td><%= link_to 'Show', evaluation_account_event %></td>
        <td><%= link_to 'Edit', edit_evaluation_account_event_path(evaluation_account_event) %></td>
        <td><%= link_to 'Destroy', evaluation_account_event, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Evaluation account event', new_evaluation_account_event_path %>
