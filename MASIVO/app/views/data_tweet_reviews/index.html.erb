<h1>Listing data_tweet_reviews</h1>

<table>
  <thead>
    <tr>
      <th>Geo</th>
      <th>State</th>
      <th>Geo cord</th>
      <th>City tweet</th>
      <th>Comuna tweet</th>
      <th>Sensitivity rank</th>
      <th>Tweet</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @data_tweet_reviews.each do |data_tweet_review| %>
      <tr>
        <td><%= data_tweet_review.geo %></td>
        <td><%= data_tweet_review.state %></td>
        <td><%= data_tweet_review.geo_cord %></td>
        <td><%= data_tweet_review.city_tweet %></td>
        <td><%= data_tweet_review.comuna_tweet %></td>
        <td><%= data_tweet_review.sensitivity_rank %></td>
        <td><%= data_tweet_review.tweet_id %></td>
        <td><%= link_to 'Show', data_tweet_review %></td>
        <td><%= link_to 'Edit', edit_data_tweet_review_path(data_tweet_review) %></td>
        <td><%= link_to 'Destroy', data_tweet_review, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Data tweet review', new_data_tweet_review_path %>
